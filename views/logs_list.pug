extends layout.pug

block content
    h1 Log di #{profilo}

    div
        table (class='table table-dark table-striped')
            thead (class='thead-light')
                tr
                    th Data
                    th Profilo
                    th Azione
                    th Testo
            tbody
                each log in logs
                    tr
                        td #{log.createdAt}
                        td #{log.profilo}
                        td #{log.action}
                        td #{log.testo}
                    